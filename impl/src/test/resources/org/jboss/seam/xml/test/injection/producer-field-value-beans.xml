<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="urn:java:ee"
          xmlns:test="urn:java:org.jboss.seam.xml.test.injection">
       
      
    <test:ProducerQualifier>
    	<Qualifier/>
    </test:ProducerQualifier>
    
    <test:ProducerBean>
    	<overrides/>
        <test:value>
            <Produces/>
            <test:ProducerQualifier/>
            <value>hello world</value>
        </test:value>
    </test:ProducerBean>

   <test:RecieverBean>
   		<overrides/>
   		<test:value>
   			<test:ProducerQualifier/>
   			<Inject/>
   		</test:value>
   </test:RecieverBean>
   
</beans>